<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
   "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="org.walkmod:walkmod-javalang-plugin:walker" class="org.walkmod.walkers.DefaultJavaWalker"
		singleton="false" />
	
	<bean id="org.walkmod:walkmod-javalang-plugin:string-writer" class="org.walkmod.writers.StringWriter"
		singleton="false" />
		
	<bean id="org.walkmod:walkmod-javalang-plugin:field-policy" class="org.walkmod.merge.FieldDeclarationPolicy"></bean>

</beans>